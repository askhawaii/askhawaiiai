{
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/askHawaiiAI",
        "function": "askHawaiiAI",
        "region": "us-central1"
      },
      {
        "source": "/readPlans",
        "function": "readPlans",
        "region": "us-central1"
      },
      {
        "source": "/readQuestions",
        "function": "readQuestions",
        "region": "us-central1"
      },
      {
        "source": "/readSuggestions",
        "function": "readSuggestions",
        "region": "us-central1"
      },
      {
        "source": "/readPrepositions",
        "function": "readPrepositions",
        "region": "us-central1"
      },
      {
        "source": "/readRelated",
        "function": "readRelated",
        "region": "us-central1"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "database": {
      "port": 9000
    },
    "hosting": {
      "port": 5002
    },
    "pubsub": {
      "port": 8089
    },
    "storage": {
      "port": 9199
    },
    "eventarc": {
      "port": 9299
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  },
  "remoteconfig": {}
}
